@model TeamDeleteServiceModel

@{
    ViewBag.Title = "Return Team";
}

<h2 class="text-center">@ViewBag.Title</h2>
<hr />

<form method="post">
    <div class="row">
        <div class="col-md-4">
            <img class="card-img-top" src="~/img/teams/@Model.LogoUrl" alt="Team Logo">
        </div>
        <div class="col-md-8">
            <div class="form-group">
                <label asp-for="Name" class="control-label"></label>
                <input asp-for="Name" class="form-control" disabled="disabled" />
            </div>
            <div class="form-group">
                <label asp-for="FoundedYear" class="control-label"></label>
                <input asp-for="FoundedYear" class="form-control" disabled="disabled" />
            </div>
            <div class="form-group">
                <label asp-for="CoachName" class="control-label"></label>
                <input asp-for="CoachName" class="form-control" disabled="disabled" />
            </div>
            <div class="form-group">
                <label asp-for="Information" class="control-label"></label>
                <textarea asp-for="Information" rows="2" class="form-control" disabled="disabled"></textarea>
            </div>
            <p></p>
            <div class="form-group">
                <input type="submit" value="Return" class="btn btn-danger" />
            </div>
        </div>
    </div>
</form>

